def printPairs(arr, n, sum):
    for i in range(0, n): 
        for j in range(i + 1, n ):
            if (arr[i] + arr[j] == sum):
                print("(", arr[i],
                      ",", arr[j],
                      ")", sep = "")


        
# Driver Code
arr=[-25, -10, -7, 7, 10, -3, 8, 3]
n = len(arr)
sum = 0
printPairs(arr, n, sum)
